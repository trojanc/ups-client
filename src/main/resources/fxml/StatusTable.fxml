<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="AnchorPane" fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="za.co.trojanc.ups.gui.controller.StatusTableController">
         <top>
            <fx:include fx:id="upsToolbar" source="UpsToolbar.fxml" />
         </top>
   <center>
            <TableView fx:id="statusTable" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="columnProperty" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Property" />
                <TableColumn fx:id="columnValue" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Value" />
              </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
            </TableView>
   </center>
</BorderPane>
